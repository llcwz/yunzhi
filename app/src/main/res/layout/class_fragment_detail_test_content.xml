<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">
  <TextView
      android:id="@+id/item_examine_content_title"
      android:textColor="@color/black_alpha_192"
      android:textSize="@dimen/font_16"
      android:text="xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx"
      android:layout_width="match_parent"
      android:layout_height="@dimen/len_48" />
  <ImageView
      android:id="@+id/item_examine_content_img"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content" />
  <FrameLayout
      android:id="@+id/fl_examine_btn_a"
      android:layout_width="match_parent"
      android:layout_height="@dimen/len_28">
     <TextView
         android:layout_marginLeft="@dimen/len_10"
         android:id="@+id/test_content_a"
         android:gravity="center|left"
         android:text="10"
         android:textColor="@color/black_alpha_192"
         android:textSize="@dimen/font_16"
         android:layout_width="match_parent"
         android:layout_height="match_parent" />
  </FrameLayout>
  <FrameLayout
      android:id="@+id/fl_examine_btn_b"
      android:layout_width="match_parent"
      android:layout_height="@dimen/len_28">
    <TextView
        android:layout_marginLeft="@dimen/len_10"
        android:id="@+id/test_content_b"
        android:gravity="center|left"
        android:text="10"
        android:textColor="@color/black_alpha_192"
        android:textSize="@dimen/font_16"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />
  </FrameLayout>
  <FrameLayout
      android:id="@+id/fl_examine_btn_c"
      android:layout_width="match_parent"
      android:layout_height="@dimen/len_28">
    <TextView
        android:layout_marginLeft="@dimen/len_10"
        android:id="@+id/test_content_c"
        android:gravity="center|left"
        android:text="10"
        android:textColor="@color/black_alpha_192"
        android:textSize="@dimen/font_16"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />
  </FrameLayout>
  <FrameLayout
      android:id="@+id/fl_examine_btn_d"
      android:layout_width="match_parent"
      android:layout_height="@dimen/len_28">
    <TextView
        android:layout_marginLeft="@dimen/len_10"
        android:id="@+id/test_content_d"
        android:gravity="center|left"
        android:text="10"
        android:textColor="@color/black_alpha_192"
        android:textSize="@dimen/font_16"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />
  </FrameLayout>
</LinearLayout>